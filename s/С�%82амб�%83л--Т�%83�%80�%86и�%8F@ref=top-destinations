    <script>

      (function () {
        var initData = BootstrapData.get('header-init');
        var country = initData ? initData.tld_country : "US";
        var headerContainer = document.getElementById('header');
        var newHeader = document.getElementById('new-header');
        var oldHeader = document.getElementById('old-header');
        var useNewHeader = function () {
          J(document.body).addClass('with-new-header');
          headerContainer.removeChild(oldHeader);
        };
        var useOldHeader = function () {
          headerContainer.removeChild(newHeader);
        };

        if (country === "IT" || country === "TW" || country === "CA") {
          Airbnb.ERF.deliverExperiment(
            'the_new_header_take_3',
            {
              new_header: useNewHeader,
              old_header: useOldHeader,
              treatment_unknown: useOldHeader
            }
          );
        } else {
          useOldHeader();
        }
      })();
